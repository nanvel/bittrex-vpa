{% extends "base.j2" %}
{% block head %}
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.bundle.min.js"></script>
{% endblock %}
{% block title %}Analysis{% endblock %}
{% block content %}
<div style="height: 400px;"><canvas id="vpa_chart"></canvas></div>
<script>
    var ctx = document.getElementById("vpa_chart").getContext("2d");
    var vpaChart = new Chart(ctx,{
        type: 'bar',
        data: {{ data|safe }},
        options: {
            animation: false
        }
    });
</script>
{% endblock %}
